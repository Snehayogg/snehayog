{
  "name": "snehayog-backend",
  "version": "1.0.0",
  "description": "Snehayog Backend with Ultra-Fast Video Processing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:worker": "node workers/videoWorker.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "setup": "npm install && npm run setup:ffmpeg",
    "setup:ffmpeg": "node scripts/setup-ffmpeg.js",
    "process:existing": "node scripts/process-existing-videos.js",
    "migrate:cloudinary": "node scripts/migrate-cloudinary.js",
    "migrate:cloudinary:dry": "node scripts/migrate-cloudinary.js --dry-run",
    "fix:urls": "node scripts/fix-malformed-urls.js",
    "fix:urls:dry": "node scripts/fix-malformed-urls.js --dry-run",
    "analyze:videos": "node scripts/migrateCloudinaryToR2.js",
    "cleanup:failed": "node scripts/migrateCloudinaryToR2.js --delete-failed",
    "test:like": "node scripts/test-like-endpoint.js",
    "test:like:dryrun": "node scripts/test-like-endpoint-dryrun.js",
    "get:video:id": "node scripts/get-test-video-id.js",
    "check:likes": "node scripts/check-like-requests.js",
    "monitor:likes": "node scripts/monitor-like-requests.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.972.0",
    "@upstash/redis": "^1.36.2",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.66.6",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "firebase-admin": "^13.6.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^13.10.0",
    "node-cron": "^4.2.1",
    "rate-limit-redis": "^4.3.1",
    "razorpay": "^2.9.6",
    "redis": "^5.9.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "video-processing",
    "quality-optimization",
    "ultra-fast-loading",
    "adaptive-streaming",
    "hls",
    "cloudinary",
    "ffmpeg"
  ],
  "author": "Snehayog Team",
  "license": "MIT"
}
