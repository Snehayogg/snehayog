{
  "name": "snehayog-backend",
  "version": "1.0.0",
  "description": "Snehayog Backend with Ultra-Fast Video Processing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "setup": "npm install && npm run setup:ffmpeg",
    "setup:ffmpeg": "node scripts/setup-ffmpeg.js",
    "process:existing": "node scripts/process-existing-videos.js",
    "migrate:cloudinary": "node scripts/migrate-cloudinary.js",
    "migrate:cloudinary:dry": "node scripts/migrate-cloudinary.js --dry-run",
    "fix:urls": "node scripts/fix-malformed-urls.js",
    "fix:urls:dry": "node scripts/fix-malformed-urls.js --dry-run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "razorpay": "^2.9.6",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "video-processing",
    "quality-optimization",
    "ultra-fast-loading",
    "adaptive-streaming",
    "hls",
    "cloudinary",
    "ffmpeg"
  ],
  "author": "Snehayog Team",
  "license": "MIT"
}
